{
  "name": "gensx-copilot-chrome-extension",
  "version": "1.0.0",
  "description": "Chrome extension for Genie - AI-powered web page interaction assistant",
  "main": "dist/background.js",
  "scripts": {
    "dev": "concurrently \"pnpm watch:web\" \"pnpm watch:gensx\" \"pnpm watch:extension\"",
    "watch:web": "cd genie-web && pnpm dev",
    "watch:gensx": "gensx start gensx/workflows.ts",
    "build:extension": "cd extension && webpack --mode=production",
    "watch:extension": "cd extension && webpack --mode=development --watch",
    "clean": "rm -rf extension/dist && rm -rf genie-web/.next && rm -rf .gensx/dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "deploy:gensx": "npx gensx deploy -e OPENAI_API_KEY -e ANTHROPIC_API_KEY -e GROQ_API_KEY -e TAVILY_API_KEY gensx/workflows.ts"
  },
  "keywords": [
    "chrome-extension",
    "gensx",
    "ai",
    "copilot",
    "web-automation",
    "browser-assistant"
  ],
  "author": "GenSX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gensx-inc/gensx"
  },
  "homepage": "https://gensx.dev",
  "devDependencies": {
    "@types/chrome": "^0.1.4",
    "@types/html-to-text": "^9.0.4",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.19.11",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.24",
    "@ai-sdk/provider": "^1.1.3",
    "@gensx/client": "^0.1.9",
    "@gensx/core": "^0.5.4",
    "@gensx/storage": "^0.2.3",
    "@gensx/vercel-ai": "^0.3.3",
    "@medv/finder": "^4.0.2",
    "@tavily/core": "^0.5.10",
    "ai": "^4.3.19",
    "europa": "^6.0.0",
    "html-to-text": "^9.0.5",
    "html2canvas-pro": "^1.5.11",
    "jquery": "^3.7.1",
    "serialize-error": "^11.0.3",
    "zod": "3.25.76"
  }
}
