{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "dist",
    "jsx": "react-jsx",
    "jsxImportSource": "@gensx/core",
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "types": ["@gensx/core", "node"]
  },
  "include": ["*.ts", "*.tsx"],
  "exclude": ["node_modules", "dist"]
}
