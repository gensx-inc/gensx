{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "dist",
    "jsx": "react-jsx",
    "jsxImportSource": "@gensx/core",
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "types": ["@gensx/core", "node"],
    "baseUrl": ".",
    "paths": {
      "@gensx/core/jsx-runtime": [
        "./node_modules/@gensx/core/dist/cjs/jsx-runtime"
      ]
    }
  },
  "include": ["*.ts", "*.tsx"],
  "exclude": ["node_modules", "dist"]
}
