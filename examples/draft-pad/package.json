{
  "name": "draft-pad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:next\" \"pnpm run dev:gensx\"",
    "dev:next": "next dev -p 3100",
    "dev:gensx": "npx gensx@latest start ./gensx/workflows.ts",
    "build": "next build",
    "build:typescript": "tsc --noEmit",
    "deploy": "pnpm run deploy-workflow",
    "deploy-workflow": "./scripts/deploy.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "test": "GENSX_ORG=gensx vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.0.0",
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/azure": "^2.0.15",
    "@ai-sdk/cohere": "^2.0.3",
    "@ai-sdk/deepseek": "^1.0.7",
    "@ai-sdk/google": "^2.0.6",
    "@ai-sdk/groq": "^2.0.10",
    "@ai-sdk/mistral": "^2.0.5",
    "@ai-sdk/openai": "^1.3.24",
    "@ai-sdk/xai": "^2.0.7",
    "@gensx/client": "^0.1.10",
    "@gensx/core": "^0.5.5",
    "@gensx/react": "^0.2.7",
    "@gensx/vercel-ai": "^0.4.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/diff-match-patch": "^1.0.36",
    "@types/dom-mediacapture-record": "^1.0.22",
    "@vercel/analytics": "^1.5.0",
    "ai": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "diff-match-patch": "^1.0.5",
    "groq-sdk": "^0.30.0",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.12",
    "next": "15.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "tailwind-merge": "^3.3.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
