{
  "name": "draft-pad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:gensx\"",
    "dev:next": "next dev --turbopack",
    "dev:gensx": "npx gensx@latest start ./gensx/workflows.ts",
    "build": "next build",
    "build:typescript": "tsc --noEmit",
    "deploy": "npm run deploy-workflow",
    "deploy-workflow": "./scripts/deploy.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next",
    "test": "GENSX_ORG=gensx vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.11",
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/azure": "^1.3.24",
    "@ai-sdk/cohere": "^1.2.10",
    "@ai-sdk/deepseek": "^0.2.15",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/xai": "^1.2.17",
    "@gensx/client": "^0.1.2",
    "@gensx/core": "^0.4.6",
    "@gensx/react": "^0.1.2",
    "@gensx/vercel-ai": "^0.2.4",
    "@radix-ui/react-slot": "^1.2.3",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.0",
    "next": "15.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "tailwind-merge": "^3.3.1",
    "zod": "3.25.75"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
