{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "jsx": "react-jsx",
    "jsxImportSource": "@gensx/core",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@gensx/core/jsx-runtime": [
        "./node_modules/@gensx/core/dist/cjs/jsx-runtime"
      ]
    }
  },
  "include": ["*.tsx", "*.ts"],
  "exclude": ["node_modules", "dist"]
}
