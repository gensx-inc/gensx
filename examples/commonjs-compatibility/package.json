{
  "name": "@examples/commonjs-compatibility",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon",
    "start:ts": "tsx index.ts",
    "start": "node ./dist/index.js",
    "start:pure": "node ./pure-commonjs.js",
    "build": "tsc",
    "build:jsx": "tsc cjs-jsx.tsx --outDir dist --module commonjs --target es2022",
    "start:jsx": "node dist/cjs-jsx.js",
    "test": "pnpm run build:jsx && node test/verify-commonjs.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@gensx/core": "^0.4.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22",
    "nodemon": "^3.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
