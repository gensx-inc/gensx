{
  "name": "@examples/vercel-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "Example of using GenSX with the Vercel AI SDK",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "start": "npx gensx start ./src/workflows.ts",
    "build": "tsc",
    "deploy": "npx gensx deploy -e OPENAI_API_KEY ./src/workflows.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@gensx/core": "workspace:*",
    "@gensx/vercel-ai": "workspace:*",
    "ai": "^5.0.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
