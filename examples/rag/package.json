{
  "name": "@examples/rag",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "Retrieval Augmented Generation with GenSX and the useSearch hook",
  "scripts": {
    "dev": "pnpm build && node --enable-source-maps dist/index.js",
    "start": "npx gensx start ./src/workflows.ts",
    "build": "tsc -p tsconfig.build.json",
    "deploy": "npx gensx deploy -e OPENAI_API_KEY ./src/workflows.ts",
    "prepare": "ts-patch install -s"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@gensx/core": "alpha",
    "@gensx/storage": "0.0.12",
    "@gensx/vercel-ai": "alpha",
    "ai": "^4.2.10",
    "openai": "^4.87.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22",
    "ts-function-decorator": "^0.0.1",
    "ts-function-decorator-ls": "^0.0.1",
    "ts-patch": "^3.3.0",
    "typescript": "^5.7.2"
  }
}
