{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "compile": "deno compile --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --include=../gensx/dist/templates main.ts",
    "compile:win": "deno compile --target x86_64-pc-windows-msvc --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --include=../gensx/dist/templates -o gensx-cli-windows.exe main.ts",
    "compile:linux": "deno compile --target x86_64-unknown-linux-gnu --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --include=../gensx/dist/templates -o gensx-cli-linux main.ts",
    "compile:macos-x64": "deno compile --target x86_64-apple-darwin --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --include=../gensx/dist/templates -o gensx-cli-macos-x64 main.ts",
    "compile:macos-arm64": "deno compile --target aarch64-apple-darwin --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --include=../gensx/dist/templates -o gensx-cli-macos-arm64 main.ts",
    "compile:all": "deno task compile:win && deno task compile:linux && deno task compile:macos-x64 && deno task compile:macos-arm64"
  },
  "compilerOptions": {
    "lib": ["deno.window", "deno.unstable", "dom", "esnext"],
    "types": ["npm:@types/node"]
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@gensx/core": "../gensx-core/dist/esm/index.js",
    "@hono/node-server": "npm:@hono/node-server",
    "ajv": "npm:ajv",
    "axios": "npm:axios",
    "builtin-modules": "npm:builtin-modules",
    "chokidar": "npm:chokidar",
    "commander": "npm:commander",
    "consola": "npm:consola",
    "enquirer": "npm:enquirer",
    "figures": "npm:figures",
    "find-up": "npm:find-up",
    "form-data": "npm:form-data",
    "hono": "npm:hono",
    "ini": "npm:ini",
    "ink": "npm:ink",
    "ink-spinner": "npm:ink-spinner",
    "ink-select-input": "npm:ink-select-input",
    "ink-text-input": "npm:ink-text-input",
    "node:buffer": "node:buffer",
    "node:crypto": "node:crypto",
    "node:os": "node:os",
    "node:readline": "node:readline",
    "open": "npm:open",
    "ora": "npm:ora",
    "picocolors": "npm:picocolors",
    "react": "npm:react",
    "@common.js/serialize-error": "npm:@common.js/serialize-error",
    "typescript-json-schema": "npm:typescript-json-schema",
    "typescript": "npm:typescript",
    "ulidx": "npm:ulidx",
    "zod-to-json-schema": "npm:zod-to-json-schema",
    "zod": "npm:zod"
  }
}
