{
  "name": "@gensx/cli",
  "version": "0.1.4",
  "description": "`GenSX command line tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/gensx-inc/gensx.git",
    "directory": "packages/gensx-cli"
  },
  "bin": {
    "gensx": "./dist/run.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp -r src/templates dist/",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsx ./bin/run.ts",
    "start:node": "node ./bin/run",
    "package": "pkg -t $TARGET --compress GZip --out-path dist/pkg ./dist/run.cjs",
    "package:macos": "TARGET=node22-macos-arm64 pnpm package"
  },
  "keywords": [
    "typescript",
    "gensx",
    "cli"
  ],
  "author": "GenSX Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@types/node": "^20.12.12",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@yao-pkg/pkg": "^6.3.0",
    "typescript": "^5.4.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "commander": "^11.0.0",
    "consola": "^3.2.3",
    "create-gensx": "workspace:*",
    "form-data": "^4.0.0",
    "glob": "^11.0.1",
    "ini": "^5.0.0",
    "open": "^10.0.3",
    "ora": "^8.2.0",
    "picocolors": "^1.0.1",
    "rollup": "^4.9.6",
    "typescript-json-schema": "^0.63.0",
    "undici": "^6.6.2"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "pkg": {
    "scripts": "dist/run.cjs"
  }
}
