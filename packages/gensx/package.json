{
  "name": "gensx",
  "version": "0.2.9",
  "description": "Build AI workflows using JSX",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx-dev-runtime.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "gensx": "./dist/run-cli.js"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "jsx",
    "workflow"
  ],
  "author": "GenSX Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@gensx/cli": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.17.11",
    "@vitest/coverage-istanbul": "^2.1.8",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
