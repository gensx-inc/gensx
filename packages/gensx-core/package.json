{
  "name": "@gensx/core",
  "version": "0.3.5",
  "description": "Build AI workflows using JSX.",
  "type": "module",
  "main": "./dist/es5/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gensx-inc/gensx.git",
    "directory": "packages/gensx-core"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/es5/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "require": "./dist/es5/jsx-runtime.js",
      "import": "./dist/jsx-runtime.js",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "require": "./dist/es5/jsx-dev-runtime.js",
      "import": "./dist/jsx-dev-runtime.js",
      "default": "./dist/jsx-dev-runtime.js"
    }
  },
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "./dist/jsx-runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "./dist/jsx-dev-runtime.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:modern && pnpm run build:es5",
    "build:modern": "tsc -p tsconfig.build.json",
    "build:es5": "tsc -p tsconfig.es5.json",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "ai",
    "jsx",
    "workflow"
  ],
  "author": "GenSX Team",
  "license": "Apache-2.0",
  "dependencies": {
    "ini": "^5.0.0",
    "serialize-error": "^12.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@types/node": "catalog:packages",
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
