{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo",
    "jsxImportSource": "@",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@/jsx-runtime": ["src/jsx-runtime.ts"],
      "@/jsx-dev-runtime": ["src/jsx-dev-runtime.ts"]
    }
  },
  "include": ["src/**/*", "vitest.config.ts", "tests/**/*", "vite.config.ts"],
  "exclude": ["node_modules", "dist"]
}
