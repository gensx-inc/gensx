{
  "name": "@gensx/gensx-cloud-mcp",
  "version": "0.0.1",
  "description": "Model Context Protocol server for GenSX Cloud.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gensx-inc/gensx.git",
    "directory": "packages/gensx-cloud-mcp"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "gensx",
    "jsx",
    "mcp",
    "model context protocol"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "GenSX Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "catalog:packages",
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@gensx/core": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "tsx": "catalog:",
    "zod": "catalog:"
  }
}
